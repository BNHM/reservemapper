{
  "name": "angular-seed",
  "private": true,
  "version": "0.0.0",
  "description": "A starter project for AngularJS",
  "repository": "https://github.com/angular/angular-seed",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.8.8",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-replace": "^0.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-usemin": "^0.3.28",
    "http-server": "^0.11.1",
    "rimraf": "^2.6.1"
  },
  "scripts": {
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'app/bower_components', 'junction') } catch (e) { }\"",
    "update-deps": "npm update",
    "postupdate-deps": "bower update",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -c-1 ./app",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "update-index-async": "node -e \"var fs=require('fs'),indexFile='app/index-async.html',loaderFile='app/bower_components/angular-loader/angular-loader.min.js',loaderText=fs.readFileSync(loaderFile,'utf-8').split(/sourceMappingURL=angular-loader.min.js.map/).join('sourceMappingURL=bower_components/angular-loader/angular-loader.min.js.map'),indexText=fs.readFileSync(indexFile,'utf-8').split(/\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/).join('//@@NG_LOADER_START@@\\n'+loaderText+'    //@@NG_LOADER_END@@');fs.writeFileSync(indexFile,indexText);\""
  },
  "dependencies": {
    "clean-css": "^4.2.2",
    "@bower_components/angular": "angular/bower-angular#1.6.8",
    "@bower_components/angular-bootstrap": "angular-ui/bootstrap-bower#^2.5.0",
    "@bower_components/angular-data-grid": "angular-data-grid/angular-data-grid.github.io#*",
    "@bower_components/angular-loader": "angular/bower-angular-loader#^1.6.8",
    "@bower_components/angular-route": "angular/bower-angular-route#^1.6.8",
    "@bower_components/angular-sanitize": "angular/bower-angular-sanitize#~1",
    "@bower_components/angular-spinner": "Attrash-Islam/bower-angular-spinner#^1.0.1",
    "@bower_components/bootstrap": "twbs/bootstrap#^3.3.7",
    "@bower_components/bootstrap-sortable": "drvic10k/bootstrap-sortable#^2.1.0",
    "@bower_components/d3": "mbostock-bower/d3-bower#^4.13.0",
    "@bower_components/font-awesome": "FortAwesome/Font-Awesome#^4.7.0",
    "@bower_components/html5-boilerplate": "h5bp/html5-boilerplate#^6.0.1",
    "@bower_components/jquery": "jquery/jquery-dist#>=1.6",
    "@bower_components/jquery-ui": "components/jqueryui#^1.12.1",
    "@bower_components/leaflet": "Leaflet/Leaflet#^1.3.0",
    "@bower_components/leaflet-spin": "makinacorpus/Leaflet.Spin#^1.1.0",
    "@bower_components/leaflet.markercluster": "Leaflet/Leaflet.markercluster#^1.2.0",
    "@bower_components/modernizr": "Modernizr/Modernizr#^3.5.0",
    "@bower_components/moment": "moment/moment#>= 2.3.1",
    "@bower_components/ng-csv": "asafdav/ng-csv#^0.3.6",
    "@bower_components/ngModal": "adamalbrecht/ngModal#^1.2.2",
    "@bower_components/ngSanitize": "gdi2290/ngSanitize#^0.0.2",
    "@bower_components/spin.js": "fgnass/spin.js#^2.3.1"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
